alias kafka-add-topic='kafka-topics --create --zookeeper localhost:2181 --replication-factor 1 --partitions 1 --topic twitter-network'
alias kafka-topics='kafka-topics --list --zookeeper localhost:2181'
alias kafka-cp='kafka-console-producer --broker-list localhost:9092 --topic twitter-network'
alias kafka-cc='kafka-console-consumer -bootstrap-server localhost:9092 -topic twitter-network'
alias zookeeper='zookeeper-server-start.bat %ZOOKEEPER_HOME%/conf/zoo.cfg'
alias kafka='kafka-server-start.bat %KAFKA_HOME%/config/server.properties'
alias producer='python producer.py'
alias consumer='python consumer.py'
alias flaskdev='FLASK_ENV=development flask run'
